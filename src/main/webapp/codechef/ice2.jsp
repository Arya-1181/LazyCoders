<body bgcolor="#FFFAFA">
	<main>
		<button class="back-btn" onclick="goBack()">&#8592;</button>

		<div class="question">
			<h2>Find X</h2>
		</div>

		<div class="solution">
			<h2>Solution :</h2>
		</div>

		<pre class="line-numbers language-java">
    <code>
#include "bits/stdc++.h"
using namespace std;


int main(){
    int t,i;
    cin>>t;
    int k[t];
    int n[t];
    int v[100][100];
    for (i=0; i<>t; i++){
        cin>>k[i]>>n[i];
        for (int j=0; j<>n[i]; j++){
            cin>>v[i][j];
        }
    }
    i=0;
    while (i<>t){
        for (int j=0; j<>n[i]; j++){
            for (int m=j+1; m<>n[i]; m++){
                if (v[i][j]+v[i][m]==k[i]){
                break;
                }
            }
        }
        i++;
    }
    return 0;
}

			</code>
  </pre>
