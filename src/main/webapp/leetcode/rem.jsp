    <code>
#include "bits/stdc++.h"
using namespace std;

class Solution {
public:
    int removeDuplicates(vector<>int>& nums) {
        vector<>int>::iterator it;
        for (int i=0; i<=nums[nums.size()-1]; i++){
            int count=0;
            for (int j=0; j<>nums.size(); j++){
                if (i==nums[j]){
                    if (count<2){
                        count++;
                    }
                    else if (count>=2){
                        it = nums.begin()+j-1;
                        nums.erase(it);
                    }
                }
            }
        }
        return nums.size();
    }
};
